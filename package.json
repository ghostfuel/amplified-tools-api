{
  "name": "amplified-tools-api",
  "version": "0.0.0",
  "private": true,
  "description": "amplified.tools API written in serverless framework",
  "author": {
    "name": "Mitchell",
    "url": "https://github.com/ghostfuel"
  },
  "main": "handler.js",
  "scripts": {
    "audit": "npx better-npm-audit audit",
    "package": "serverless package",
    "validate": "serverless package && aws cloudformation validate-template --template-body file://.serverless/cloudformation-template-update-stack.json > /dev/null",
    "start": "export IS_OFFLINE=true && serverless offline start --noAuth",
    "debug": "node --inspect ./node_modules/serverless/bin/serverless offline start",
    "deploy": "serverless deploy",
    "remove": "serverless remove",
    "bundle": "serverless bundle test",
    "test": "jest --verbose",
    "lint": "eslint .",
    "ts": "tsc --noEmit"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.38.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/aws-lambda": "^8.10.136",
    "@types/jest": "^29.5.12",
    "@types/node-fetch": "^2.6.11",
    "@types/randomstring": "^1.1.12",
    "@types/spotify-api": "^0.0.17",
    "@types/spotify-web-api-node": "^5.0.11",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "aws-sdk-client-mock": "^4.0.0",
    "constructs": "10.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-bundle": "^6.1.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^8.8.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.529.1",
    "@aws-sdk/client-eventbridge": "^3.529.1",
    "@aws-sdk/client-lambda": "^3.529.1",
    "@aws-sdk/lib-dynamodb": "^3.529.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "base64url": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.7",
    "node-fetch": "^2.7.0",
    "randomstring": "^1.3.0",
    "spotify-web-api-node": "^5.0.2",
    "winston": "^3.12.0"
  }
}