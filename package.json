{
  "name": "amplified-tools-api",
  "version": "0.0.0",
  "private": true,
  "description": "amplified.tools API written in serverless framework",
  "author": {
    "name": "Mitchell",
    "url": "https://github.com/ghostfuel"
  },
  "main": "handler.js",
  "scripts": {
    "audit": "npx better-npm-audit audit",
    "package": "serverless package",
    "validate": "serverless package && aws cloudformation validate-template --template-body file://.serverless/cloudformation-template-update-stack.json > /dev/null",
    "start": "export IS_OFFLINE=true && serverless offline start --noAuth",
    "debug": "node --inspect ./node_modules/serverless/bin/serverless offline start",
    "deploy": "serverless deploy",
    "remove": "serverless remove",
    "bundle": "serverless bundle test",
    "test": "jest --verbose",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^28.1.6",
    "@types/node-fetch": "^2.6.2",
    "@types/randomstring": "^1.1.8",
    "@types/spotify-api": "^0.0.17",
    "@types/spotify-web-api-node": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "aws-sdk-client-mock": "^1.0.0",
    "dotenv": "^16.0.1",
    "eslint": "8.21.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^26.6.3",
    "serverless-bundle": "^5.4.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^8.8.1",
    "ts-jest": "^26.5.6"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.142.0",
    "@aws-sdk/client-eventbridge": "^3.142.0",
    "@aws-sdk/client-lambda": "^3.142.0",
    "@aws-sdk/lib-dynamodb": "^3.142.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "base64url": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "node-fetch": "^2.6.7",
    "randomstring": "^1.2.2",
    "spotify-web-api-node": "^5.0.2",
    "winston": "^3.8.1"
  }
}